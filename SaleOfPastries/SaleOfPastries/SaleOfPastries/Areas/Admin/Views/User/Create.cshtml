@model User

@{ 
    ViewBag.Title = "Create";
}

<form asp-action="Create" asp-controller="User" method="post">
    <div class="form-group">
        <label class="control-lable" asp-for="FullName"></label>
        <input type="text" value="" asp-for="FullName" class="form-control" />
    </div>
    <div class="form-group">
        <label class="control-lable" asp-for="Email"></label>
        <input type="text" value="" asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label class="control-lable" asp-for="Password"></label>
        <input type="password" value="" asp-for="Password" class="form-control" />
    </div>
    <div class="form-group">
        <label class="control-lable" asp-for="Phone"></label>
        <input type="text" value="" asp-for="Phone" class="form-control" />
    </div>
    <div class="form-group">
        <label class="control-lable" asp-for="Address"></label>
        <input type="text" value="" asp-for="Address" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-default" />
    </div>
</form>