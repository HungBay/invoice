@model IEnumerable<invoice.Models.viewModel.InvoiceView>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MOTA_SP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GIA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SOLUONG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ID_KHACHHANG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DIA_CHI_KHACH_HANG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TEN_KHACH_HANG)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ID_HOADON)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NGAYBAN)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MOTA_SP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GIA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SOLUONG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ID_KHACHHANG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DIA_CHI_KHACH_HANG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TEN_KHACH_HANG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ID_HOADON)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NGAYBAN)
            </td>
            <td>
                @*<a asp-action="Edit" asp-route-id="@item.ID_SP">Edit</a> |*@
                <a asp-action="Details" asp-route-id="@item.ID_SP">Details</a>
                @*<a asp-action="Delete" asp-route-id="@item.ID_SP">Delete</a>*@
            </td>
        </tr>
}
    </tbody>
</table>
